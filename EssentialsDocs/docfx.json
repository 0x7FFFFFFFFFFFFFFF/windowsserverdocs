{
   "build": {
      "content": [
         {
            "files": [
               "**/*.md"
            ],
            "exclude": [
               "**/obj/**",
               "**/includes/**"
            ]
         }
      ],
      "resource": [
         {
            "files": [
               "**/*.png",
               "**/*.jpg"
            ],
            "exclude": [
               "**/obj/**",
               "**/includes/**"
            ]
         }
      ],
      "overwrite": [],
      "externalReference": [],
      "globalMetadata": {
         "_op_documentIdPathDepotMapping": {
            "./": {
               "depot_name": "WS.EssentialsDocs"
            }
         }
      },
      "fileMetadata": {
         "ms.prod": {
            "get-started/*.md": "windows-server",
            "install/*.md": "windows-server",
            "manage/*.md": "windows-server",
            "migrate/*.md": "windows-server",
            "support/*.md": "windows-server",
            "use/*.md": "windows-server"
         },
         "ms.technology": {
            "get-started/*.md": "essentials",
            "install/*.md": "essentials",
            "manage/*.md": "essentials",
            "migrate/*.md": "essentials",
            "support/*.md": "essentials",
            "use/*.md": "essentials"
         }
      },
       "template": [],
       "dest": "window-server-docs/essentials",
       "markdownEngineName": "markdig"
   }
}