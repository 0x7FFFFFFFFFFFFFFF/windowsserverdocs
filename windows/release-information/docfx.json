{
	"build": {
		"content": [{
			"files": [
				"**/*.md",
				"**/*.yml"
			],
			"exclude": [
				"**/obj/**",
				"**/includes/**",
				"_themes/**",
				"_themes.pdf/**",
				"**/docfx.json",
				"_repo.en-us/**",
				"README.md",
				"LICENSE",
				"LICENSE-CODE",
				"ThirdPartyNotices.md"
			]
		}],
		"resource": [{
			"files": [
				"**/*.png",
				"**/*.jpg"
			],
			"exclude": [
				"**/obj/**",
				"**/includes/**",
				"_themes/**",
				"_themes.pdf/**",
				"**/docfx.json",
				"_repo.en-us/**"
			]
		}],
		"overwrite": [],
		"externalReference": [],
		"globalMetadata": {
			"breadcrumb_path": "/windows/release-information/breadcrumb/toc.json",
			"extendBreadcrumb": true,
			"feedback_system": "None",
			"_op_documentIdPathDepotMapping": {
				"./": {
					"depot_name": "TechNet.release-information",
					"folder_relative_path_in_docset": "./"
				}
			}
		},
		"fileMetadata": {},
		"template": [],
		"dest": "rel-info",
		"markdownEngineName": "markdig"
	}
}
